/* login */
#loader {padding:0; margin:0; position:absolute; top:0; left:0; width:100%; height:100%; background:#ffffff; opacity: 0.98; z-index:999; vertical-align:middle;}
#loaderInner {position: absolute; top: 50%; left: 50%; width: 32px; height: 32px; margin-top: -16px; margin-left: -16px; background-image: url(/res/img/dcp/loader.gif); background-repeat: no-repeat;}
#loginBody .text {padding-top:10px; text-align:left; width:500px;}
#loginBody .text li {list-style: none; margin:0 0 5px 0; background:url(/res/img/dcp/util.png) no-repeat 0 5px; padding-left:7px; color:#252525;}
#loginBody .text span {color:#00387f;}
#loginBody .text a {color:#ba000c;}


/* dcp table */
table.dcpTable {border:1px solid #cbcbcb; border-top:2px solid #48bcff; width:100%; border-collapse:collapse; border-spacing:0; word-wrap:break-word;}
table.dcpTable .tr {border-top:2px solid #a20000; z-index: 999; }
table.dcpTable th {padding:7px 0; border:1px solid #cbcbcb; font-weight:bold; text-align:center; vertical-align:middle; line-height:16px; background-color:#e5e7ea;}
table.dcpTable .th {padding:7px 0; border:1px solid #cbcbcb; font-weight:bold; text-align:center; vertical-align:middle; line-height:16px; background-color:#e5e7ea;}
table.dcpTable tr:nth-child(odd) {background: #f9f9f9;}
table.dcpTable td {padding:7px; line-height:17px; border:1px solid #cbcbcb; text-align:center; vertical-align:middle;} 
table.dcpTable td.al {text-align:left; padding-left:12px;}
table.dcpTable td.ar {text-align:right;}
table.dcpTable td label {line-height:24px; vertical-align:middle;}
table.dcpTable td input {vertical-align:middle;}
table.dcpTable td select {height:22px;}
table.dcpTable td img.ui-datepicker-trigger {margin-left:5px;vertical-align:middle;}
table.dcpTable.non-odd tr:nth-child(odd) {background: none;}
table.dcpTable .bg {background-color:#f8e0e4 !important;}
table.dcpTable .selectTd { background-color: #f47171; color:white;}

/* paging */
.paging a {padding-left:5px;}
.paging a img {vertical-align: middle;}
.paging a.on {font-weight: bold; font-size: 17px;}


